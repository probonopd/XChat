#: src/common/outbound.c:1677
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Neznámy argument '%s' ignorovaný."

#: src/common/outbound.c:2384
msgid "No such plugin found.\n"
msgstr "Plugin sa nenašiel.\n"

#: src/common/outbound.c:2389 src/fe-gtk/plugingui.c:181
msgid "That plugin is refusing to unload.\n"
msgstr "Plugin sa odmieta odobrať.\n"

#: src/common/outbound.c:2534
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <meno> <akcia>, pridá tlačidlo pod zoznam používateľov"

#: src/common/outbound.c:2536
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <príkaz>, pošle príkaz do všetkých kanálov, v ktorých ste"

#: src/common/outbound.c:2538
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <príkaz>, pošle príkaz na všetky servery, na ktorých ste"

#: src/common/outbound.c:2539
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<dôvod>], označí vás ako neprítomného"

#: src/common/outbound.c:2541
msgid "BAN <mask> [<bantype>], bans everyone matching the mask from the current channel. If they are already on the channel this doesn't kick them (needs chanop)"
msgstr "BAN <maska> [<typ>], zakáže každého, kto odpovedá maske, z aktuálneho kanála. Ak už sú prihlásený v kanáli, nevykopne ich to (musíte byť chanop)"

#: src/common/outbound.c:2543
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, vyčistí aktuálne textové okno"

#: src/common/outbound.c:2544
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, zavrie aktuálne okno alebo záložku"

#: src/common/outbound.c:2547
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <kód>, nájde kód krajiny, napr. sk = Slovensko"

#: src/common/outbound.c:2549
msgid "CTCP <nick> <message>, send the CTCP message to nick, common messages are VERSION and USERINFO"
msgstr "CTCP <prezývka> <správa>, pošle správu CTCP prezývka, obvyklé správy sú VERSION a USERINFO"

#: src/common/outbound.c:2551
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, opustí aktuálny kanál a hneď sa vráti"

#: src/common/outbound.c:2553
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <prezývka>          - príjme ponúkaný súbor\n"
"DCC SEND <prezývka> <súbor>  - pošle niekomu súbor\n"
"DCC LIST                - zobrazí zoznam DCC\n"
"DCC CHAT <prezývka>         - ponúkne niekomu DCC CHAT\n"
"DCC CLOSE <typ> <prezývka> <súbor>         príklad:\n"
"         /dcc close send janko subor.tar.gz"

#: src/common/outbound.c:2563
msgid "DEHOP <nick>, removes chanhalf-op status from the nick on the current channel (needs chanop)"
msgstr "DEHOP <prezývka>, odstráni stav chanhalf-op pre prezývku v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2565
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <meno>, odstráni tlačidlo pod zoznamom používateľov"

#: src/common/outbound.c:2567
msgid "DEOP <nick>, removes chanop status from the nick on the current channel (needs chanop)"
msgstr "DEOP <prezývka>, odstráni stav chanop pre prezývku v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2569
msgid "DEVOICE <nick>, removes voice status from the nick on the current channel (needs chanop)"
msgstr "DEVOICE <prezývka>, odstráni stav voice pre prezývku v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2570
msgid "DISCON, Disconnects from server"
msgstr "DISCON, odpojí od serveru"

#: src/common/outbound.c:2571
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <prezývka|hostiteľ|ip>, nájde IP adresu používateľa"

#: src/common/outbound.c:2572
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <text>, Vytlačí text lokálne"

#: src/common/outbound.c:2575
msgid "EXEC [-o] <command>, runs the command. If -o flag is used then output is sent to current channel, else is printed to current text box"
msgstr "EXEC [-o] <príkaz>, spustí príkaz. Ak je použitý prepínač -o, výstup sa posiela do aktuálneho kanálu, inak sa vytlačí do aktuálneho textového poľa"

#: src/common/outbound.c:2577
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, pošle procesu SIGCONT"

#: src/common/outbound.c:2580
msgid "EXECKILL [-9], kills a running exec in the current session. If -9 is given the process is SIGKILL'ed"
msgstr "EXECKILL [-9], zabije programy bežiace v aktuálnom sedení. Ak je zadané -9, pošle sa signál SIGKILL"

#: src/common/outbound.c:2582
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, pošle procesu SIGSTOP"

#: src/common/outbound.c:2583
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, pošle procesu dáta na štandardný vstup"

#: src/common/outbound.c:2587
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, vyčistí aktuálnu frontu serveru pre posielanie"

#: src/common/outbound.c:2589
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr "GATE <hostiteľ> [<port>], nastaví proxy na hostiteľa, port je štandardne 23"

#: src/common/outbound.c:2594
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr "HOP <prezývka>, nastaví stav chanhalf-op pre prezývku (vyžaduje chanop)"

#: src/common/outbound.c:2596
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <maska> <typy..> <možnosti..>\n"
"    maska - maska ignorovaných hostiteľov, napr.: *!*@*.aol.com\n"
"    typy - typy ignorovaný dát, jedna alebo všetky z:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    možnosti - NOSAVE, QUIET"

#: src/common/outbound.c:2603
msgid "INVITE <nick> [<channel>], invites someone to a channel, by default the current channel (needs chanop)"
msgstr "INVITE <prezývka> [<kanál>], pozve niekoho do kanála, štandardne do aktuálneho kanála (vyžaduje chanop)"

#: src/common/outbound.c:2604
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <kanál>, prejde do kanálu"

#: src/common/outbound.c:2606
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr "KICK <prezývka>, vykopne prezývku z aktuálneho kanála (vyžaduje chanop)"

#: src/common/outbound.c:2608
msgid "KICKBAN <nick>, bans then kicks the nick from the current channel (needs chanop)"
msgstr "KICKBAN <prezývka>, zakáže a potom vykopne prezývku z aktuálneho kanála (vyžaduje chanop)"

#: src/common/outbound.c:2610
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, vynúti novú kontrolu lag"

#: src/common/outbound.c:2612
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <reťazec>, hľadá reťazec v texte."

#: src/common/outbound.c:2614
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <súbor>, načíta skript Perl"

#: src/common/outbound.c:2617
msgid "MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr "MDEHOP, hromadne zruší stav všetkým chanhalf-op v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2619
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr "MDEOP, hromadne zruší stav všetkým chanop v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2621
msgid "ME <action>, sends the action to the current channel (actions are written in the 3rd person, like /me jumps)"
msgstr "ME <akcia>, pošle akciu do aktuálneho kanála (akcie sa píšu v tretej osobe, napríklad /me skáče)"

#: src/common/outbound.c:2623
msgid "MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr "MKICK, hromadne vykopne každého okrem vás z aktuálneho kanála (vyžaduje chanop)"

#: src/common/outbound.c:2625
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr "MOP, hromadne nastaví stav všetkým používateľom v aktuálnom kanáli (vyžaduje chanop)"

#: src/common/outbound.c:2626
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <prezývka> <správa>, pošle súkromnú správu"

#: src/common/outbound.c:2629
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, vypíše zoznam prezývok v aktuálnom kanáli"

#: src/common/outbound.c:2631
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <prezývka> <správa>, pošle poznámku CTCP"

#: src/common/outbound.c:2632
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <hostiteľ> [<port>]"

#: src/common/outbound.c:2633
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <prezývka>, nastaví vašu prezývku"

#: src/common/outbound.c:2636
msgid "NOTICE <nick/channel> <message>, sends a notice. Notices are a type of message that should be auto reacted to"
msgstr "NOTICE <prezývka/kanál> <správa>, pošle poznámku. Poznámky sú typom správy, na ktoré by sa malo automaticky reagovať"

#: src/common/outbound.c:2638
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr "NOTIFY [<prezývka>], vypíše váš zoznam upozornení, alebo niekoho do tohto zoznamu pridá"

#: src/common/outbound.c:2640
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr "OP <prezývka>, dá stav chanop prezývke (vyžaduje chanop)"

#: src/common/outbound.c:2642
msgid "PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<kanál>] [<dôvod>], opustí kanál, štandardne aktuálny"

#: src/common/outbound.c:2644
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <prezývka | kanál>, pošle CTCP ping na prezývku alebo kanál"

#: src/common/outbound.c:2646
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <prezývka>, otvorí nové okno pre súkromné správy niekomu"

#: src/common/outbound.c:2648
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<dôvod>], odpojí sa od aktuálneho serveru"

#: src/common/outbound.c:2650
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <text>, pošle text tak ako je na server"

#: src/common/outbound.c:2653
msgid "RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /RECONNECT to reconnect to the current server or with /RECONNECT ALL to reconnect to all the open servers"
msgstr "RECONNECT [-ssl] [<hostiteľ>] [<port>] [<heslo>], môže sa použiť aj ako /RECONNECT pre znovupripojenie k aktuálnemu serveru alebo ako /RECONNECT ALL pre znovupripojenie ku všetkým otvoreným serverom"

#: src/common/outbound.c:2656
msgid "RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT to reconnect to the current server or with /RECONNECT ALL to reconnect to all the open servers"
msgstr "RECONNECT [<hostiteľ>] [<port>] [<heslo>], môže sa použiť aj ako /RECONNECT pre znovupripojenie k aktuálnemu serveru alebo ako /RECONNECT ALL pre znovupripojenie ku všetkým otvoreným serverom"

#: src/common/outbound.c:2658
msgid "RECV <text>, send raw data to xchat, as if it was received from the irc server"
msgstr "RECV <text>, odošle xchatu čisté data, ako keby boli prijaté z irc serveru"

#: src/common/outbound.c:2661
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <text>, pošle text objektu v aktuálnom okne"

#: src/common/outbound.c:2664
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <hostiteľ> <port> <kanál>, pripojí sa a navštívi kanál"

#: src/common/outbound.c:2667
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <hostiteľ> <port> <kanál>, pripojí sa a navštívi kanál"

#: src/common/outbound.c:2671
msgid "SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr "SERVER [-ssl] <hostiteľ> [<port>] [<heslo>], pripojí sa na server, štandardný port je 6667 pre obyčajné pripojenia, 9999 pre ssl pripojenia."

#: src/common/outbound.c:2674
msgid "SERVER <host> [<port>] [<password>], connects to a server, the default port is 6667"
msgstr "/SERVER <hostiteľ> [<port>] [<heslo>], pripojí sa na server, štandardný port je 6667"

#: src/common/outbound.c:2676
msgid "SET <variable> [<value>]"
msgstr "SET <premenná> [<hodnota>]"

#: src/common/outbound.c:2679
msgid "TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr "TOPIC [<téma>], nastaví tému, ak je zadaná, inak zobrazí aktuálnu tému"

#: src/common/outbound.c:2681
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <maska> [<maska>...], zruší zákaz pre zadané masky."

#: src/common/outbound.c:2682
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <maska> [QUIET]"

#: src/common/outbound.c:2683
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <súbor>, načíta plugin alebo skript"

#: src/common/outbound.c:2686
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr "VOICE <prezývka>, priradí niekomu stav voice (vyžaduje chanop)"

#: src/common/outbound.c:2688
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <správa>, napíše správu do všetkých kanálov"

#: src/common/outbound.c:2690
msgid "WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr "WALLCHOP <správa>, pošle správu všetkým so stavom chanop v aktuálnom kanáli"

#: src/common/outbound.c:2723
#, c-format
msgid "Usage: %s\n"
msgstr "Použitie: %s\n"

#: src/common/outbound.c:2728
msgid ""
"\n"
"No help available on that command.\n"
msgstr ""
"\n"
"Pre tento príkaz nie je žiadna pomoc.\n"

#: src/common/outbound.c:2734
msgid "No such command.\n"
msgstr "Neznámy príkaz.\n"

#: src/common/outbound.c:3058
msgid "Bad arguments for user command.\n"
msgstr "Chybné argumenty pre používateľský príkaz.\n"

#: src/common/outbound.c:3222
msgid "Too many recursive usercommands, aborting."
msgstr "Príliš veľa vnorených používateľských príkazov, končím."

#: src/common/outbound.c:3295
msgid "Unknown Command. Try /help\n"
msgstr "Neznámy príkaz. Skúste použiť /help\n"

#: src/common/plugin.c:349 src/common/plugin.c:379
msgid "No xchat_plugin_init symbol; is this really an xchat plugin?"
msgstr "Nemôžem nájsť xchat_plugin_init symbol, je to naozaj plugin do xchatu?"

#: src/common/server.c:887
#, c-format
msgid ""
"Cannot resolve hostname %s\n"
"Check your IP Settings!\n"
msgstr ""
"Nemôžem zistiť meno hostiteľa %s\n"
"Skontrolujte si IP nastavenia!\n"

#: src/common/server.c:892
msgid "Proxy traversal failed.\n"
msgstr "Spojenie cez proxy zlyhalo.\n"

#: src/common/textevents.h:6
msgid "-%C10-%C11-%O$t$1 added to notify list."
msgstr "-%C10-%C11-%O$t$1 pridaný do zoznamu upozornení."

#: src/common/textevents.h:9
msgid "-%C10-%C11-%O$t$1 Banlist: %C3$4 %C4$2 %C5$3%O"
msgstr "-%C10-%C11-%O$t$1 Zoznam zákazov: %C3$4 %C4$2 %C5$3%O"

#: src/common/textevents.h:12
msgid "-%C10-%C11-%O$tCannot join%C11 %B$1 %O(You are banned)."
msgstr "-%C10-%C11-%O$tPríchod zakázaný do %C11 %B$1 %O(Ste v zozname zákazov)."

#: src/common/textevents.h:15
msgid "-%C10-%C11-%O$t$1 is now known as $2"
msgstr "-%C10-%C11-%O$t$1 sa premenoval na $2"

#: src/common/textevents.h:18
msgid "%C13*%O$t$1 $2%O"
msgstr "%C13*%O$t$1 $2%O"

#: src/common/textevents.h:21
msgid "%C13*%O$t%C8%B$1%B%O $2%O"
msgstr "%C13*%O$t%C8%B$1%B%O $2%O"

#: src/common/textevents.h:24
msgid "-%C10-%C11-%O$t$1 sets ban on $2"
msgstr "-%C10-%C11-%O$t$1 nastavil zákaz pre $2"

#: src/common/textevents.h:27
msgid "-%C10-%C11-%O$tChannel $1 created on $2"
msgstr "-%C10-%C11-%O$tKanál $1 vytvorený na $2"

#: src/common/textevents.h:30
msgid "-%C10-%C11-%O$t%C11$1%O removes channel half-operator status from %C11$2"
msgstr "-%C10-%C11-%O$t%C11$1%O odstránil stav half-operator kanála pre %C11$2"

#: src/common/textevents.h:33
msgid "-%C10-%C11-%O$t%C11$1%O removes channel operator status from %C11$2"
