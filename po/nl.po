#: src/common/outbound.c:1650
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Onbekend argument '%s' genegeerd."

#: src/common/outbound.c:2314
msgid "No such plugin found.\n"
msgstr "Plugin niet gevonden.\n"

#: src/common/outbound.c:2317 src/fe-gtk/plugingui.c:128
msgid "That plugin is refusing to unload.\n"
msgstr "Die plugin weigert te ontladen.\n"

#: src/common/outbound.c:2451
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <naam> <actie>, voegt een knop toe onder de gebruikerslijst"

#: src/common/outbound.c:2453
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <opdr>, stuurt een opdracht naar alle kanalen waar je in zit"

#: src/common/outbound.c:2455
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <opdr>, stuurt een opdracht naar alle servers waarop je bent ingelogd"

#: src/common/outbound.c:2456
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<reden>], zegt dat je weg bent"

#: src/common/outbound.c:2458
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <masker> [<bantype>], bant iedereen die voldoet aan het masker uit het "
"huidige kanaal. Als ze al in het huidige kanaal zitten worden ze niet "
"ge'kickt (chanop vereist)"

#: src/common/outbound.c:2460
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <masker> [<masker>], de opgegeven maskers weer toelaten"

#: src/common/outbound.c:2461
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Maakt het huidige tekstvenster schoon"

#: src/common/outbound.c:2462
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Sluit het huidige venster/tabblad"

#: src/common/outbound.c:2465
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <persoon> <bericht>, een CTCP bericht naar de persoon sturen. Veel "
"voorkomende berichten zijn VERSION en USERINFO"

#: src/common/outbound.c:2467
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <code>, zoek een landcode, bijvoorbeeld au = australië"

#: src/common/outbound.c:2469
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, het huidige kanaal verlaten en direct terugkeren"

#: src/common/outbound.c:2471
msgid ""
"\n"
"DCC GET <nick>          - receive an offered file\n"
"DCC SEND <nick> <file>  - send a file to someone\n"
"DCC LIST                - show DCC list\n"
"DCC CHAT <nick>         - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <persoon>         - een aangeboden bestand ontvangen\n"
"DCC SEND <persoon> <bestand>  - een bestand naar iemand sturen\n"
"DCC LIST           - de DCC lijst laten zien\n"
"DCC CHAT <persoon>        - iemand DCC CHAT aanbieden\n"
"DCC CLOSE <type> <persoon> <bestand>     voorbeeld:\n"
"      /dcc close send janjanssen bestand.tar.g"

#: src/common/outbound.c:2480
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <naam>, een knop van de gebruikerslijst wissen"

#: src/common/outbound.c:2482
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <persoon>, de kanaal-halfoperator status van de persoon in het huidige "
"kanaal verwijderen (kanaal-operator vereist)"

#: src/common/outbound.c:2484
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <persoon>, kanaal-operatorstatus van de persoon in het huidige kanaal "
"halen (kanaal-operator vereist)"

#: src/common/outbound.c:2486
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <persoon>, de stemstatus van de persoon in het huidige kanaal halen "
"(kanaal-operator vereist)"

#: src/common/outbound.c:2487
msgid "DISCON, Disconnects from server"
msgstr "DISCON, de verbinding met de server verbreken"

#: src/common/outbound.c:2488
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <persoon|host|ip>, het IP-adres van een gebruiker vinden"

#: src/common/outbound.c:2489
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <tekst>, lokaal een tekst afdrukken"

#: src/common/outbound.c:2492
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <opdracht>, een opdracht starten. Als de -o vlag meegegeven is, "
"wordt de uitvoer naar het huidige kanaal gestuurt, anders naar de huidige "
"tekstomgeving"

#: src/common/outbound.c:2494
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], een lopende exec in de huidige sessie stoppen. Als -9 is "
"meegegeven wordt het process geSIGKILLd"

#: src/common/outbound.c:2496
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, een process SIGSTOP sturen"

#: src/common/outbound.c:2497
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, het process SIGCONT sturen"

#: src/common/outbound.c:2498
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, gegevens naar stdin sturen"

#: src/common/outbound.c:2502
#, fuzzy
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, de huidige server zijn verzendrij doorspoelen"

#: src/common/outbound.c:2504
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <host> [<poort>], verbinding maken door een proxy, de poort is "
"standaard 23"

#: src/common/outbound.c:2507
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""

#: src/common/outbound.c:2509
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""

#: src/common/outbound.c:2516
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <persoon> [<kanaal>], iemand uitnodigen een kanaal te openen, "
"standaard het huidige kanaal (kanaal operator vereist)"

#: src/common/outbound.c:2517
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <kanaal>, het kanaal binnengaan"

#: src/common/outbound.c:2519
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <persoon>, de persoon van het huidige kanaal kicken (kanaal operator "
"vereist)"

#: src/common/outbound.c:2521
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAR <persoon>, een persoon eerst bannen en dan kicken van het huidige "
"kanaal (kanaal operator vereist)"

#: src/common/outbound.c:2523
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, een nieuwe controle op lag forceren"

#: src/common/outbound.c:2525
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <tekst>, een tekst zoeken in de buffer"

#: src/common/outbound.c:2527
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <bestand>, laadt een plugin of script"

#: src/common/outbound.c:2530
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr "MDEHOP, deopt alle kanaal-halfops in het kanaal (chanop vereist)"

#: src/common/outbound.c:2532
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr "MDEHOP, deopt alle kanaal-ops in het kanaal (chanop vereist)"

#: src/common/outbound.c:2534
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, kickt iedereen in het huidige kanaal behalve jezelf (chanop vereist)"

#: src/common/outbound.c:2536
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <actie>, stuurt de actie naar het huidige kanaal (acties worden "
"geschreven in de 3e persoon, bijv: /me springt)"

#: src/common/outbound.c:2537
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <persoon> <bericht>, een privéberichtje sturen"

#: src/common/outbound.c:2540
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, geef de lijst met personen op het huidige kanaal"

#: src/common/outbound.c:2542
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <persoon> <bericht>, een CTCP waarschuwing sturen"

#: src/common/outbound.c:2543
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <hostnaam> [<poort>]"

#: src/common/outbound.c:2544
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <bijnaam>, je bijnaam instellen"

#: src/common/outbound.c:2547
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <persoon/kanaal> <bericht>, een waarschuwing sturen. Waarschuwingen "
"zijn het soort berichten waarop automatisch op gereageerd zou moeten worden."

#: src/common/outbound.c:2549
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<persoon>], een lijst geven van de waarschuwingslijst of iemand er "
"aan toevoegen"

#: src/common/outbound.c:2551
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <persoon>, de kanaal operator status aan de persoon geven (kanaal "
"operator vereist)"

#: src/common/outbound.c:2553
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr ""
"PART [<kanaal>] [<reden>], het kanaal verlaten, standaard het huidige kanaal."

#: src/common/outbound.c:2555
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <persoon | kanaal>, een persoon of kanaal CTCP pingen"

#: src/common/outbound.c:2557
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <persoon>, een nieuw privaat berichtvenster naar iemand openen"

#: src/common/outbound.c:2559
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<reden>], verbinding met de huidige server verbreken"

#: src/common/outbound.c:2561
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <tekst>, tekst in ruw formaat naar de server sturen"

#: src/common/outbound.c:2564
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<host>] [<poort>] [<paswoord>], Kan net als /RECONNECT "
"gebruikt worden om opnieuw te verbinden met de huidige server, of om met /"
"RECONNECT ALL met alle open servers een nieuwe verbinding te maken"

#: src/common/outbound.c:2567
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<host>] [<poort>] [<paswoord>], Kan net als /RECONNECT gebruikt "
"worden om opnieuw te verbinden met de huidige server, of om met /RECONNECT "
"ALL met alle open servers een nieuwe verbinding te maken"

#: src/common/outbound.c:2569
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <tekst>, ruwe gegevens naar X-Chat sturen, alsof het verstuurd was door "
"de irc server"

#: src/common/outbound.c:2572
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <tekst>, de tekst naar het object in het huidige venster sturen"

#: src/common/outbound.c:2573
msgid "SET <variable> [<value>]"
msgstr "SET <variabele> [<waarde>]"

#: src/common/outbound.c:2577
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN [-ssl] <host> <poort> <kanaal>, verbinding maken en een kanaal "
"openen"

#: src/common/outbound.c:2580
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN  <host> <poort> <kanaal>, verbinding maken en een kanaal openen"

#: src/common/outbound.c:2584
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <host> [<poort>] [<paswoord>], verbinding maken met een "
"server, de standaard poort is 6667 voor gewone verbindingen, 9999 voor ssl "
"verbindingen"

#: src/common/outbound.c:2587
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <host> [<poort>] [<paswoord>], verbinding maken met een server, de "
"standaard poort is 6667 voor gewone verbindingen"

#: src/common/outbound.c:2590
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<topic>], het onderwerp opeven als topic gespecifiëerd is, anders het "
"huidige onderwerp laten zien"

#: src/common/outbound.c:2591
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <mask> [QUIET]"

#: src/common/outbound.c:2592
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <naam>, een plugin of script laden"

#: src/common/outbound.c:2595
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <bericht>, een bericht naar alle kanaaloperatoren op het huidige "
"kanaal"

#: src/common/outbound.c:2597
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <bericht>, het bericht naar alle kanalen sturen"

#: src/common/outbound.c:2599
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr ""
"VOICE <persoon>, geef de stemstatus aan iemand (kanaal operator vereist)"

