#: ../src/common/outbound.c:1781
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Argumento descoñecido '%s' ignorado."

#: ../src/common/outbound.c:2559
msgid "No such plugin found.\n"
msgstr "Non se atopou ningún complemento.\n"

#: ../src/common/outbound.c:2564 ../src/fe-gtk/plugingui.c:185
msgid "That plugin is refusing to unload.\n"
msgstr "Ese complemento négase a ser desactivado.\n"

#: ../src/common/outbound.c:2721
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <nome> <acción>, engade un botón debaixo da lista de usuarios"

#: ../src/common/outbound.c:2723
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <cmd>, envía unha orde a todos os canles aos que se uniu"

#: ../src/common/outbound.c:2725
msgid "ALLCHANL <cmd>, sends a command to all channels you're in"
msgstr "ALLCHANL <cmd>, envía un comando a todos os canles aos que está conectado"

#: ../src/common/outbound.c:2727
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <cmd>, envía un comando a todos os servidores aos que está conectado"

#: ../src/common/outbound.c:2728
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<razón>], establece súa ausencia"

#: ../src/common/outbound.c:2730
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr "BAN <máscara> [<tipo de veto>], veta a todos aqueles que concorden coa máscara do canle actual. Se xa están no canle isto non os expulsa (é necesario ser operador do canle)"

#: ../src/common/outbound.c:2732
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Limpa o texto da fiestra actual"

#: ../src/common/outbound.c:2733
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Pecha a fiestra/solapa actual"

#: ../src/common/outbound.c:2736
msgid "COUNTRY [-s] <code|wildcard>, finds a country code, eg: au = australia"
msgstr "COUNTRY [-s] <code|wildcard>, busca o código de país, ex: au = australia"

#: ../src/common/outbound.c:2738
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr "CTCP <apodo> <mensaxe>, envía a mensaxe CTCP ao apodo, as mensaxes comúns son VERSION e USERINFO"

#: ../src/common/outbound.c:2740
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, márchase do canle actual e inmediatamente volve a entrar"

#: ../src/common/outbound.c:2742
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <apodo>                     - acepta un ficheiro ofrecido\n"
"DCC SEND [-maxcps=#] <apodo> [ficheiro] - envía un ficheiro a alguén\n"
"DCC LIST                           - amosa a lista DCC\n"
"DCC CHAT <apodo>                    - ofrece o DCC CHAT a alguén\n"
"DCC CLOSE <tipo> <apodo> <ficheiro>         exemplo:\n"
"         /dcc close send xoándapena ficheiro.tar.gz"

#: ../src/common/outbound.c:2752
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr "DEHOP <apodo>, elimina o estado de semi-operador do canle ao apodo no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2754
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <nome>, borra un botón de debaixo da lista de usuarios"

#: ../src/common/outbound.c:2756
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr "DEOP <apodo>, quita o estado de operador do canle ao apodo no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2758
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr "DEVOICE <apodo>, quita o estado de voz do apodo no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2759
msgid "DISCON, Disconnects from server"
msgstr "DISCON, Desconéctase do servidor"

#: ../src/common/outbound.c:2760
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <apodo|equipo|ip>, Busca o enderezo IP dun usuario"

#: ../src/common/outbound.c:2761
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <texto>, Imprime texto localmente"

#: ../src/common/outbound.c:2764
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr "EXEC [-o] <comando>, executa o comando. Se se usa a bandeira -o entón a saída envíase ao canle actual, se non, imprímese na caixa de texto actual"

#: ../src/common/outbound.c:2766
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, envía o proceso SIGCONT"

#: ../src/common/outbound.c:2769
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr "EXECKILL [-9], mata un executable correndo na sesión actual. Se se indica -9 o proceso é matado pola forza"

#: ../src/common/outbound.c:2771
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, envía ao proceso o sinal de deterse"

#: ../src/common/outbound.c:2772
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, envía os datos á entrada estándar do proceso"

#: ../src/common/outbound.c:2776
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, borra a cola de envío actual do servidor"

#: ../src/common/outbound.c:2778
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr "GATE <equipo> [<porto>], utiliza un proxy a través dun equipo, o porto por defecto é o 23"

#: ../src/common/outbound.c:2784
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr "HOP <apodo>, da o estado de semi-operador ao apodo (necesita ser operador do canle)"

#: ../src/common/outbound.c:2786
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <máscara> <tipos..> <opcións..>\n"
"    máscara - máscara de equipos a ignorar, ex: *!*@*.aol.com\n"
"    tipos - tipos de datos a ignorar, un ou todos:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    opcións - NOSAVE, QUIET"

#: ../src/common/outbound.c:2793
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr "INVITE <apodo> [<canle>], invita a alguén a un canle, por omisión o canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2794
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <canle>, entra ao canle"

#: ../src/common/outbound.c:2796
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr "KICK <apodo>, expulsa ao apodo do canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2798
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr "KICKBAN <apodo>, veta e logo expulsa ao apodo do canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2801
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, forza unha nova comprobación de retardo"

#: ../src/common/outbound.c:2803
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <cadea>, busca o texto no búfer"

#: ../src/common/outbound.c:2805
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <ficheiro>, carga un complemento ou script"

#: ../src/common/outbound.c:2808
msgid "MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr "MDEHOP, Quita masivamente o estado de semi-operador no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2810
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr "MDEOP, Quita masivamente o estado de operador no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2812
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr "ME <acción>, envía a acción ao canle actual (as accións están escritas en terceira persoa, como /me jumps)"

#: ../src/common/outbound.c:2814
msgid "MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr "MKICK, Expulsa masivamente a todos excepto a vostede no canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2817
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr "MOP, Otorga masivamente o estado de operador do canle actual (necesita ser operador do canle)"

#: ../src/common/outbound.c:2818
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <apodo> <mensaxe>, envía unha mensaxe privado"

#: ../src/common/outbound.c:2821
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, Lista os apodos do canle actual"

#: ../src/common/outbound.c:2823
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <apodo> <mensaxe>, Envía unha noticia CTCP"

#: ../src/common/outbound.c:2824
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <nome do equipo> [<porto>]"

#: ../src/common/outbound.c:2825
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <apodo>, establece seu apodo"

#: ../src/common/outbound.c:2828
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr "NOTICE <apodo/canle> <mensaxe>, envía unha noticia. As noticias son un tipo de mensaxe que debería ser auto publicado"

#: ../src/common/outbound.c:2830
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr "NOTIFY [<apodo>], lista súa lista de notificacións ou agrega a alguén a ela"

#: ../src/common/outbound.c:2832
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr "OP <apodo>, otorga o estado de operador do canle ao apodo (necesita ser operador do canle)"

#: ../src/common/outbound.c:2834
msgid "PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<canle>] [<razón>], abandona o canle, por defecto o canle actual"

#: ../src/common/outbound.c:2836
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <apodo | canle>, Envía pings CTCP ao apodo ou canle"

#: ../src/common/outbound.c:2838
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <apodo>, abre unha nova fiestra privada de mensaxes hacia alguén"

#: ../src/common/outbound.c:2840
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<razón>], desconéctase do servidor actual"

#: ../src/common/outbound.c:2842
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <texto>, envía o texto en formato chan hacia o servidor"

#: ../src/common/outbound.c:2845
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr "RECONNECT [-ssl] [<equipo>] [<porto>] [<contrasinal>], Pode ser chamado só como /RECONNECT para reconectarse ao servidor actual ou con /RECONNECT ALL para reconectarse a todos os servidores abertos"

#: ../src/common/outbound.c:2848
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr "RECONNECT [<equipo>] [<porto>] [<contrasinal>], Pode ser chamado só como /RECONNECT para reconectarse ao servidor actual ou con /RECONNECT ALL para reconectarse a todos os servidores abertos"

#: ../src/common/outbound.c:2850
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr "RECV <text>, envía datos en bruto hacia xchat como se estos fosen recibidos desde o servidor IRC"

#: ../src/common/outbound.c:2853
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <texto>, envía texto hacia un obxecto da fiestra actual"

#: ../src/common/outbound.c:2856
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <equipo> <porto> <canle>, Conéctao e entra nun canle"

#: ../src/common/outbound.c:2859
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <equipo> <porto> <canle>, Conéctao e entra nun canle"

#: ../src/common/outbound.c:2863
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr "SERVER [-ssl] <equipo> [<porto>] [<contrasinal>], conéctao ao servidor, o porto por defecto é 6667 para conexións normais e 9999 para conexións ssl"

#: ../src/common/outbound.c:2866
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr "SERVER <equipo> [<porto>] [<contrasinal>], conéctao ao servidor, por defecto o porto é 6667"

#: ../src/common/outbound.c:2868
msgid "SET [-quiet] <variable> [<value>]"
msgstr "SET [-quiet] <variable> [<valor>]"

#: ../src/common/outbound.c:2869
msgid "SETCURSOR [-|+]<position>"
msgstr "SETCURSOR [-|+]<posición>"

#: ../src/common/outbound.c:2873
msgid "TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr "TOPIC [<tópico>], estable o tópico se se provee un, se non mostra o tópico actual "

#: ../src/common/outbound.c:2875
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <máscara> [<máscara>...], reincorpora as máscaras especificadas."

#: ../src/common/outbound.c:2876
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <máscara> [QUIET]"

#: ../src/common/outbound.c:2877
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <nome>, desactiva un complemento ou script"

#: ../src/common/outbound.c:2878
msgid "URL <url>, opens a URL in your browser"
msgstr "URL <url>, abre unha URL no seu navegador"

#: ../src/common/outbound.c:2881
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr "VOICE <apodo>, otorga o estado de voz a alguén (necesita ser operador do canle)"

#: ../src/common/outbound.c:2883
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <mensaxe>, escribe a mensaxe a todos os canles"

#: ../src/common/outbound.c:2885
msgid "WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr "WALLCHOP <mensaxe>, envía a mensaxe a todos os operadores do canle no canle actual"

#: ../src/common/outbound.c:2918
#, c-format
msgid "Usage: %s\n"
msgstr "Uso: %s\n"

#: ../src/common/outbound.c:2923
msgid ""
"\n"
"No help available on that command.\n"
msgstr ""
"\n"
"Non hai axuda dispoñible para ese comando.\n"

#: ../src/common/outbound.c:2929
msgid "No such command.\n"
msgstr "Non existe ese comando.\n"

#: ../src/common/outbound.c:3253
msgid "Bad arguments for user command.\n"
msgstr "Argumentos erróneos para o comando de usuario.\n"

#: ../src/common/outbound.c:3417
msgid "Too many recursive usercommands, aborting."
msgstr "Demasiados comandos recursivos de usuario, abortando."

#: ../src/common/outbound.c:3494
msgid "Unknown Command. Try /help\n"
msgstr "Comando descoñecido. Tente con /help\n"

#: ../src/common/plugin.c:371 ../src/common/plugin.c:412
msgid "No xchat_plugin_init symbol; is this really an xchat plugin?"
msgstr "Non hai o símbolo xchat_plugin_init; é realmente un complemento de xchat?"

#: ../src/common/server.c:596
msgid "Are you sure this is a SSL capable server and port?\n"
msgstr "Está seguro de que é un servidor e porto SSL?\n"

#: ../src/common/server.c:947
#, c-format
msgid ""
"Cannot resolve hostname %s\n"
"Check your IP Settings!\n"
msgstr ""
"Non se pode resolver o nome do equipo %s\n"
"Comprobe súa configuración de IP!\n"

#: ../src/common/server.c:952
msgid "Proxy traversal failed.\n"
msgstr "Fallou o proxy traversal.\n"

#: ../src/common/servlist.c:632
#, c-format
msgid "Cycling to next server in %s...\n"
msgstr "Ciclando ao seguinte servidor en %s...\n"

#: ../src/common/servlist.c:1044
#, c-format
msgid ""
"Warning: \"%s\" character set is unknown. No conversion will be applied for "
"network %s."
msgstr "Aviso: O conxunto de caracteres \"%s\" é descoñecido. Non se aplicará conversión para a rede %s."

#: ../src/common/textevents.h:6
msgid "%C22*%O$t$1 added to notify list."
msgstr "%C22*%O$t$1 engadido á lista de notificacións."

#: ../src/common/textevents.h:9
msgid "%C22*%O$t$1 Banlist:%C19 $4%C20 $2%C21 $3"
msgstr "%C22*%O$t$1 Lista de vetados:%C19 $4%C20 $2%C21 $3"

#: ../src/common/textevents.h:12
msgid "%C22*%O$tCannot join%C26 %B$1 %O(You are banned)."
msgstr "%C22*%O$tNon se pode entrar%C26 %B$1 %O(Está vetado)."

#: ../src/common/textevents.h:18
msgid "%C22*%O$t$1 is now known as $2"
msgstr "%C22*%O$t$1 é agora coñecido como $2"

#: ../src/common/textevents.h:21
msgid "%C18*$t$1%O $2"
msgstr "%C18*$t$1%O $2"

#: ../src/common/textevents.h:24
msgid "%C21*%O$t%C21%B$1%O%C21 $2"
msgstr "%C21*%O$t%C21%B$1%O%C21 $2"

#: ../src/common/textevents.h:27
msgid "%C22*%O$t$1 sets ban on $2"
