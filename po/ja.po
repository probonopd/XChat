msgid "%C13*%O$t$1 $2%O"
msgstr "%C13*%O$t$1 $2%O"

#: src/common/textevents.h:21
msgid "%C13*%O$t%C8%B$1%B%O $2%O"
msgstr "%C13*%O$t%C8%B$1%B%O $2%O"

