msgstr "No existeix tal comandament.\n"

#: src/common/outbound.c:2909
msgid "Bad arguments for user command.\n"
msgstr "Arguments erronis per al comandament emprat.\n"

#: src/common/outbound.c:3019
msgid "Too many recursive usercommands, aborting."
msgstr "Masses comandaments d'usuari recursius, avortant."

#: src/common/outbound.c:3085
msgid "Unknown Command. Try /help\n"
msgstr "Comandament desconegut. Proveu amb /help\n"

