#: src/common/ignore.c:398
#, c-format
msgid "You are being MSG flooded from %s, setting gui_auto_open_dialog OFF.\n"
msgstr ""
"Bây giờ bạn tràn ngập MSG từ %s nên TẮT tùy chọn tự động mở đôi thoại giao "
"diện  (gui_auto_open_dialog).\n"

#: src/common/notify.c:400
#, c-format
msgid "  %-20s online\n"
msgstr "  %-20s trực tuyến\n"

#: src/common/notify.c:402
#, c-format
msgid "  %-20s offline\n"
msgstr "  %-20s ngoạị tuyến\n"

#: src/common/outbound.c:70
msgid "No channel joined. Try /join #<channel>\n"
msgstr "Chưa vào kênh nào: hãy thử lệnh /join #<kênh>\n"

#: src/common/outbound.c:76
msgid "Not connected. Try /server <host> [<port>]\n"
msgstr "Chưa kêt nối được: hãy thử lệnh /server <máy_chủ> [<cổng>]\n"

#: src/common/outbound.c:1375
msgid "I need /bin/sh to run!\n"
msgstr "Cần đến thư mục/tập tin này để chạy được: /bin/sh\n"

#: src/common/outbound.c:1618
msgid ""
"\n"
"Commands Available:\n"
"\n"
"  "
msgstr ""
"\n"
"Lệnh sẵn sàng:\n"
"\n"
"  "

#: src/common/outbound.c:1653
msgid ""
"\n"
"\n"
"Type /HELP <command> for more information, or /HELP -l\n"
"\n"
msgstr ""
"\n"
"\n"
"Hãy gõ lệnh /HELP <lệnh> để xem thông tin thêm (help - trợ giúp), hay /HELP -"
"l\n"
"\n"

#: src/common/outbound.c:1654
msgid ""
"User defined commands:\n"
"\n"
"  "
msgstr ""
"Lệnh tự định nghĩa:\n"
"\n"
"  "

#: src/common/outbound.c:1746
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Không biết đối số '%s' nên bỏ qua nó."

#: src/common/outbound.c:2524
msgid "No such plugin found.\n"
msgstr "Không tìm thấy trình cầm phít như vậy.\n"

#: src/common/outbound.c:2529 src/fe-gtk/plugingui.c:185
msgid "That plugin is refusing to unload.\n"
msgstr "Trình cầm phít ấy không bỏ nạp đươc.\n"

#: src/common/outbound.c:2686
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr ""
"Lệnh ADDBUTTON <tên> <hành_động> thì _thêm_ một _cái nút_ ở dưới danh sách "
"người dùng"

#: src/common/outbound.c:2688
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "Lệnh ALLCHAN <lệnh> thì gởi lệnh ấy cho _mọi kênh_ hiện có của bạn"

#: src/common/outbound.c:2690
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "Lệnh ALLSERV <lệnh> thì gởi lệnh ấy cho _mọi máy chủ_ hiện có của bạn"

#: src/common/outbound.c:2691
msgid "AWAY [<reason>], sets you away"
msgstr "Lệnh AWAY [<lý do>] lập trang thái của bạn thành _Vắng mặt_"

#: src/common/outbound.c:2693
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"Lệnh BAN <mặt_nạ> [<cách_đuổi>] thì _đuổi_ mọi người khớp với mặt nạ ấy ra "
"kênh hiện có. Cần đến quyền Thao tác (chanop) để đuổi ra người nào trên kênh "
"rồi."

#: src/common/outbound.c:2695
msgid "CLEAR, Clears the current text window"
msgstr "Lệnh CLEAR thì _xóa_ cửa sổ câu hiện có"

#: src/common/outbound.c:2696
msgid "CLOSE, Closes the current window/tab"
msgstr "Lệnh CLOSE thi _đóng_ cửa sổ/thanh hiện có"

#: src/common/outbound.c:2699
#, fuzzy
msgid "COUNTRY [-s] <code|wildcard>, finds a country code, eg: au = australia"
msgstr "Lệnh COUNTRY <mã> thì tìm thấy _quốc gia_, v.d. mã au = Úc"

#: src/common/outbound.c:2701
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"Lệnh CTCP <tên_hiệu> <tin_nhẳn> thì gởi cho tên hiệu ấy tin nhẳn CTCP ấy: "
"hai tin nhẳn thường là VERSION (phiên bản) và USERINFO (thông tin người dùng)"

#: src/common/outbound.c:2703
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "Lệnh CYCLE thì rời kênh hiện đi rồi vào lại ngay (_quay lại_)"

#: src/common/outbound.c:2705
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"Lệnh DCC (Trò chuyện trực tiếp) thường dụng:\n"
"\n"
"Lệnh DCC GET <tên_hiệu> \t\tthì chấp nhận tập tin đã đưa ra (_gọi_)\n"
"Lệnh DCC SEND [-maxcps=#] <tên_hiệu> [tập_tin]      thì _gởi_ tập tin ấy cho "
"người ấy\n"
"Lệnh DCC LIST   \t\t\t\tthì hiển thị _danh sách_ DCC\n"
"Lệnh DCC CHAT <tên_hiệu> \tthì đưa ra _trò chuyện_ DCC với người ấy\n"
"Lệnh DCC CLOSE <loại> <tên_hiệu> <tập_tin>   (_đóng_)      thí dụ:\n"
"         /dcc close send nguyenvan tập_tin.tar.gz (gởi cho người nguyenvan "
"tập_tin.tar.gz)"

#: src/common/outbound.c:2715
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"Lệnh DEHOP <tên_hiệu> thì bỏ ra tên hiệu ấy trạng thái Thao tác kênh nửa "
"(chanhalf-op) trên kênh hiện có (cần đến trạng thái Thao tác kênh) - DE (bỏ "
"ra) Half (nửa) OPerator (Thao tác viên)"

#: src/common/outbound.c:2717
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr ""
"Lệnh DELBUTTON <tên> thi _xóa bỏ cái nút_ ấy ra dưới danh sách người dùng "
"(DELete - xóa bỏ)"

#: src/common/outbound.c:2719
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"Lệnh DEOP <tên_hiệu> thì bỏ ra tên hiệu ấy trạng thái Thao tác kênh (chanop) "
"trên kênh hiện có (cần đến trạng thái Thao tác kênh (chanop)) - DE (bỏ) "
"OPerator (Thao tác viên)"

#: src/common/outbound.c:2721
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"Lệnh DEVOICE <tên_hiệu> thì _bỏ ra_ tên hiệu ấy trạng thái _nói tiếng_ trên "
"kênh hiện có (cần đến trạng thái Thao tác kênh (chanop))"

#: src/common/outbound.c:2722
msgid "DISCON, Disconnects from server"
msgstr "Lệnh DISCON thì ngắt kết nối ra máy chủ - DISCONnect (ngắt kết nối)"

#: src/common/outbound.c:2723
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr ""
"Lệnh DNS <tên_hiệu|máy_chủ|ip> thì tìm thấy địa chỉ IP của người dùng ấy - "
"Domain Name System (Hệ thống tên miền)"

#: src/common/outbound.c:2724
msgid "ECHO <text>, Prints text locally"
msgstr "Lệnh ECHO <câu> thì in ra câu ấy địa phương (_vọng_)"

#: src/common/outbound.c:2727
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"Lệnh EXEC [-o] <lệnh> thì chạy lệnh ấy. Nếu sử dụng cờ -o thì gởi dữ liệu "
"xuất cho kênh hiện có, không thì in ra dữ liệu xuất vào hộp câu hiện có - "
"EXECute (thi hành)"

#: src/common/outbound.c:2729
msgid "EXECCONT, sends the process SIGCONT"
msgstr "Lệnh EXECCONT thì gởi tiến trình SIGCONT"

#: src/common/outbound.c:2732
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"Lệnh EXECKILL [-9] thì buộc kết thức tiến trình exec đang chạy trong phiên "
"hiện có. Nếu sử dụng cờ -9 thì SIGKILL tiến trình ấy"

#: src/common/outbound.c:2734
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "Lệnh EXECSTOP thì gởi tiến trình SIGSTOP"

#: src/common/outbound.c:2735
msgid "EXECWRITE, sends data to the processes stdin"
msgstr ""
"Lệnh EXECWRITE thi gởi dữ liệu cho thiết bị gõ chuẩn (stdin) của tiến trình "
"- EXECute (thi hành) WRITE (ghi)"

#: src/common/outbound.c:2739
msgid "FLUSHQ, flushes the current server's send queue"
msgstr ""
"Lệnh FLUSHQ thì _xóa sạch hàng đợi_ gởi của máy chủ hiện có - Queue (hàng "
"đời)"

#: src/common/outbound.c:2741
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"Lệnh GATE <máy_chủ> [<cổng>] thì sử dụng máy chủ ủy nhiệm; _cổng_ mặc định "
"là 23"

#: src/common/outbound.c:2747
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"Lệnh HOP <tên_hiệu> thì đưa ra tên hiệu ấy trạng thái Thao tác kênh nửa (cần "
"đến trạng thái Thao tác kênh [chanop]) - Half (nửa) OPerator (Thao tac viên)"

#: src/common/outbound.c:2749
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"Lệnh IGNORE <mặt_nạ> <loại..> <tùy_chọn..>\n"
"    IGNORE - bỏ ra\n"
"    mặt nạ - mặt nạ máy chủ để bỏ qua, v.d.: *!*@*.aol.com\n"
"    loại - loại dữ liệu để bỏ qua, là một hay tất cả của:\n"
"            PRIV (riêng), CHAN (kênh), NOTI (thông báo), CTCP, DCC (trò "
"chuyện trực tiếp), INVI (mời), ALL (tất cả)\n"
"    tùy chọn - NOSAVE (không lưu), QUIET (im)"

#: src/common/outbound.c:2756
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"Lệnh INVITE <tên_hiệu> [<kênh>] thì _mời_ người ấy vào kênh ấy; mặc định là "
"kênh hiện có (cần đến trạng thái Thao tác kênh [chanop])"

#: src/common/outbound.c:2757
msgid "JOIN <channel>, joins the channel"
msgstr "Lệnh JOIN <kênh> thi _vào_ kênh ấy"

#: src/common/outbound.c:2759
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"Lệnh KICK <tên_hiệu> thi _đá_ người ấy ra kênh hiện có (cần đến trạng thái "
"Thao tác kênh [chanop])"

#: src/common/outbound.c:2761
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"Lệnh KICKBAN <tên_hiệu> thì _đuổi_ rồi _đá_ người ấy ra kênh hiện có (cần "
"đến trạng thái Thao tác kênh [chanop])"

#: src/common/outbound.c:2764
msgid "LAGCHECK, forces a new lag check"
msgstr "Lệnh LAGCHECK buộc _kiểm tra sự trễ_ kết nối một lần nữa"

#: src/common/outbound.c:2766
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "Lệnh LASTLOG <chuỗi> tìm kiếm chuối ấy trong bộ đệm (_bản ghi trước_)"

#: src/common/outbound.c:2768
msgid "LOAD <file>, loads a plugin or script"
msgstr "Lệnh LOAD <tập_tin> thì _nạp_ trình cầm phít hay tập lệnh"

#: src/common/outbound.c:2771
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"Lệnh MDEHOP thì bỏ trạng thái Thao tác kênh nửa (chanhalf-op) ra mọi người "
"trên kênh hiện có (cần đến trạng thái Thao tác kênh [chanop]) - Mass (số "
"nhiều) DE (bỏ) Half (nửa) OPerator (Thao tác viên)"

#: src/common/outbound.c:2773
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"Lệnh MDEOP thì bỏ trạng thái Thao tác kênh (chanop) ra mọi người trên kênh "
"hiện có (cần đến trạng thái Thao tác kênh [chanop]) - Mass (số nhiều) DE "
"(bỏ) OPerator (Thao tác viên)"

#: src/common/outbound.c:2775
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"Lệnh ME <hành_động> thì gởi hành động ấy cho kênh hiện có (v.d. \"ME jumps\" "
"có nghĩa là «Tôi nhảy») - ME (tôi)"

#: src/common/outbound.c:2777
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"Lệnh MKICK thì _đá_ mọi người (trừ bạn) ra kênh hiện có (cần đến trạng thái "
"Thao tác kênh [chanop]) - Mass (số nhiều)"

#: src/common/outbound.c:2780
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"Lệnh MOP thì đưa ra mọi người dùng trạng thái Thao tác kênh (chanop) trên "
"kênh hiện có (cần đến trạng thái Thao tác kênh [chanop]) - Mass (số nhiều) "
"OPerator (Thao tác viên)"

#: src/common/outbound.c:2781
msgid "MSG <nick> <message>, sends a private message"
msgstr ""
"Lệnh MSG <tên_hiệu> <tin_nhẳn> thì gởi _tin nhẳn_ riêng - MeSsaGe (tin nhẳn)"

#: src/common/outbound.c:2784
msgid "NAMES, Lists the nicks on the current channel"
msgstr "Lệnh NAMES thì liệt kê mọi _tên_ hiệu trên kênh hiện có"

#: src/common/outbound.c:2786
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr ""
"Lệnh NCTCP <tên_hiệu> <tin_nhẳn> thì gởi thông báo CTCP - Notice (thông báo)"

#: src/common/outbound.c:2787
msgid "NEWSERVER <hostname> [<port>]"
msgstr "Lệnh NEWSERVER <tên_máy_chủ> [<cổng>] thì lập _máy chủ mới_"

#: src/common/outbound.c:2788
msgid "NICK <nickname>, sets your nick"
msgstr ""
"Lệnh NICK <tên_hiệu> thì lập _tên hiệu_ của bạn - NICKname (tên hiệu: viết "
"tắt)"

#: src/common/outbound.c:2791
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"Lệnh NOTICE <tên_hiệu/kênh> <tin_nhản> thì gởi _thông báo_: thông báo là "
"loại tin nhẳn sẽ nhận trả lời tự động"

#: src/common/outbound.c:2793
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"Lệnh NOTIFY [<tên_hiệu>] thì liệt kê danh sách _thông báo_ của bạn, hay thêm "
"người nào vào nó"

#: src/common/outbound.c:2795
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"Lệnh OP <tên_hiệu> thì đưa ra tên hiệu ấy trạng thái Thao tác kênh (chanop) "
"(cần đến trạng thái Thao tác kênh [chanop]) - OPerator (Thao tác viên)"

#: src/common/outbound.c:2797
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr ""
"Lệnh PART [<kênh>] [<lý_do>] thì rời kênh ấy đi; mặc định là kênh hiện có - "
"dePART (rời đi)"

#: src/common/outbound.c:2799
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr ""
"Lệnh PING <tên_hiệu | kênh> thì thi hành tiến trình ping CTCP với tên hiêu "
"hay kênh ấy"

#: src/common/outbound.c:2801
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr ""
"Lệnh QUERY <tên_hiệu> thì mở một cửa sổ tin nhẳn riêng mới cho người ấy "
"(_câu hỏi_)"

#: src/common/outbound.c:2803
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "Lệnh QUIT [<lý_do>] thì ngắt kết nối ra máy chủ hiện có (_thoát_)"

#: src/common/outbound.c:2805
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "Lệnh QUOTE <câu> thì gởi câu ấy dạng thô cho máy chủ (_trích dẫn_)"

#: src/common/outbound.c:2808
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"Lệnh RECONNECT [-ssl] [<máy_chủ>] [<cổng>] [<mật_khẩu>]\n"
"có thể kết nối lại đến mấy chủ hiện có giống như chỉ đơn giản sử dụng /"
"RECONNECT, hay kết nối lại đến mọi máy chủ mở với /RECONNECT ALL (phương "
"pháp SSL bảo vậ dữ liệu sẽ chuyển giao)"

#: src/common/outbound.c:2811
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"Lệnh RECONNECT [<máy_chủ>] [<cổng>] [<mật_khẩu>]\n"
"có thể kết nối lại đến mấy chủ hiện có giống như chỉ đơn giản sử dụng /"
"RECONNECT, hay kết nối lại đến mọi máy chủ mở với /RECONNECT ALL (không có "
"bảo vệ dữ liệu với SSL)"

#: src/common/outbound.c:2813
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"Lệnh RECV <câu> thì gởi dữ liệu thô vào trình xchat, như máy chủ irc có thể "
"gởi - RECeiVe (nhận)"

#: src/common/outbound.c:2816
msgid "SAY <text>, sends the text to the object in the current window"
msgstr ""
"Lệnh SAY <câu> thì gởi câu ấy cho đối tượng trong cửa sổ hiện có (_nói_)"

#: src/common/outbound.c:2819
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"Lệnh SERVCHAN [-ssl] <máy_chủ> <cổng> <kênh> thì kết nối đến kênh ấy và vào "
"nó (phương pháp SSL bảo vệ dữ liệu sẽ chuyển giao) (phương pháp SSL bảo vệ "
"dữ liệu sẽ chuyển giao) - SERVer (máy chủ) CHANnel (kênh)"

#: src/common/outbound.c:2822
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr ""
"Lệnh SERVCHAN <máy_chủ> <cổng> <kênh> thì kết nối đến kênh ấy và vào nó "
"(không có bảo vệ dữ liệu với SSL) - SERVer (máy chủ) CHANnel (kênh)"

