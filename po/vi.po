#: src/common/notify.c:398
#, c-format
msgid "  %-20s offline\n"
msgstr "  %-20s ngoạị tuyến\n"

#: src/common/outbound.c:72
msgid "No channel joined. Try /join #<channel>\n"
msgstr "Chưa vào kênh nào: hãy thử lệnh « /join #<kênh> »\n"

#: src/common/outbound.c:78
msgid "Not connected. Try /server <host> [<port>]\n"
msgstr "Chưa kêt nối được: hãy thử lệnh « /server <máy_phục_vụ> [<cổng>] »\n"

#: src/common/outbound.c:1650
msgid "I need /bin/sh to run!\n"
msgstr "Cần đến thư « /bin/sh » để chạy được.\n"

#: src/common/outbound.c:1983
msgid "Commands Available:"
msgstr "Các lệnh sẵn sàng:"

#: src/common/outbound.c:1997
msgid "User defined commands:"
msgstr "Các lệnh tự định nghĩa:"

#: src/common/outbound.c:2013
msgid "Plugin defined commands:"
msgstr "Các lệnh do bộ cầm phít định nghĩa:"

#: src/common/outbound.c:2024
msgid "Type /HELP <command> for more information, or /HELP -l"
msgstr "Hãy gõ lệnh « /HELP <lệnh> » để xem thông tin thêm (help: trợ giúp), hay « /"
"HELP -l »."

#: src/common/outbound.c:2099
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Không biết đối số « %s » nên bỏ qua nó."

#: src/common/outbound.c:2960
msgid "No such plugin found.\n"
msgstr "Không tìm thấy bộ cầm phít như vậy.\n"

#: src/common/outbound.c:2965
#: src/fe-gtk/plugingui.c:186
msgid "That plugin is refusing to unload.\n"
msgstr "Bộ cầm phít đó không bỏ tải đươc.\n"

#: src/common/outbound.c:3200
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <tên> <hành_động>, _thêm_ một _cái nút_ <tên> thực hiện <hành "
"động> ở dưới danh sách người dùng."

#: src/common/outbound.c:3202
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <lệnh>,  gửi lệnh này đến tất cả các kênh mà bạn đang có mặt."

#: src/common/outbound.c:3204
msgid "ALLCHANL <cmd>, sends a command to all channels you're in"
msgstr "ALLCHANL <lệnh>, gởi lệnh này đến tất cả các kênh mà bạn đang có mặt."

#: src/common/outbound.c:3206
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <lệnh>, gởi lệnh này đến tất cả các máy phục vụ mà bạn đang có mặt."

#: src/common/outbound.c:3207
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<lý do>], đặt trang thái của bạn thành _Vắng mặt_"

#: src/common/outbound.c:3209
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr "BAN <bộ_lọc> [<kiểu_đuổi>], _đuổi_ mọi người khớp với bộ lọc này ra kênh "
"hiện tại. Cần quyền quan trị kệnh (chanop) để đuổi ra người nào đang trên "
"kênh."

#: src/common/outbound.c:3211
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, _xóa_ cửa sổ văn bản hiện tại."

#: src/common/outbound.c:3212
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, _đóng_ cửa sổ/thanh hiện tại."

#: src/common/outbound.c:3215
msgid "COUNTRY [-s] <code|wildcard>, finds a country code, eg: au = australia"
msgstr "COUNTRY  [-s] <mã|đại_diện>, tìm thấy _quốc gia_, v.d. mã au = Úc"

#: src/common/outbound.c:3217
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr "CTCP <tên_hiệu> <tin_nhẳn>, gởi cho <tên_hiệu> <tin nhẳn> CTCP: hai tin nhẳn "
"thường là VERSION (phiên bản) và USERINFO (thông tin người dùng)."

#: src/common/outbound.c:3219
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, rời kênh hiện tại đi rồi vào lại ngay (_quay lại_)."

#: src/common/outbound.c:3221
msgid ""
"\n"
"DCC GET <nick>                      - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file]  - send a file to someone\n"
"DCC PSEND [-maxcps=#] <nick> [file] - send a file using passive mode\n"
"DCC LIST                            - show DCC list\n"
"DCC CHAT <nick>                     - offer DCC CHAT to someone\n"
"DCC PCHAT <nick>                    - offer DCC CHAT using passive mode\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr "\n"
"DCC GET <tên_hiệu> \t\tchấp nhận tập tin đã đưa ra (_lấy_)\n"
"DCC SEND [-maxcps=#] <tên_hiệu> [tập_tin]\t\t _gởi_ tập tin này cho người có "
"tên hiệu này\n"
"DCC PSEND [-maxcps=#] <tên_hiệu> [tập_tin]\t\tthì _gởi_ tập tin này bằng chế "
"độ _bị động_\n"
"DCC LIST   \t\t\t\thiển thị _danh sách_ DCC\n"
"DCC CHAT <tên_hiệu> \tđưa ra _trò chuyện_ DCC với người có tên hiệu này\n"
"DCC PCHAT <tên_hiệu>\t\tđưa ra _trờ chuyện_ DCC bằng chế độ _bị động_\n"
"DCC CLOSE <kiểu> <tên_hiệu> <tập_tin>   (_đóng_)      thí dụ:\n"
"         </dcc close send nguyenvan tập_tin.tar.gz> (gởi cho người nguyenvan "
"tập_tin.tar.gz)"

#: src/common/outbound.c:3233
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr "DEHOP <tên_hiệu>, bỏ ra <tên hiệu> trạng thái quản trị kênh nửa (chanhalf-"
"op) trên kênh hiện tại (cần quyền quản trị kênh) — DE (bỏ ra) Half (nửa) "
"OPerator (quản trị viên)."

#: src/common/outbound.c:3235
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <tên>, _xóa bỏ cái nút_ <tên> ra dưới danh sách người dùng (DELete "
"— xóa bỏ)."

#: src/common/outbound.c:3237
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr "DEOP <tên_hiệu>, bỏ ra <tên hiệu> trạng thái quản trị kênh (chanop) trên "
"kênh hiện tại (cần quyền quản trị kênh (chanop)) — DE (bỏ) OPerator (quản "
"trị viên)."

#: src/common/outbound.c:3239
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr "DEVOICE <tên_hiệu>, _bỏ ra_ <tên hiệu> trạng thái _tiếng nói_ trên kênh hiện "
"tại (cần quyền quản trị kênh [chanop])."

#: src/common/outbound.c:3240
msgid "DISCON, Disconnects from server"
msgstr "DISCON, ngắt kết nối ra máy phục vụ — DISCONnect (ngắt kết nối)."

#: src/common/outbound.c:3241
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <tên_hiệu|máy_phục_vụ|ip>, tìm thấy địa chỉ IP của <ten hiệu> — Domain "
"Name System (Hệ thống tên miền)."

#: src/common/outbound.c:3242
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <đoạn>, in ra <đoạn> một cách địa phương (_vọng_)."

#: src/common/outbound.c:3245
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr "EXEC [-o] <lệnh>, _thực hiện_ <lệnh>. Nếu thêm cờ « -o » thì gởi dữ liệu "
"xuất cho kênh hiện tại, nếu không thì in ra nó vào hộp văn bản hiện tại — "
"EXECute (thực hiện)."

#: src/common/outbound.c:3247
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, gởi tiến trình SIGCONT."

#: src/common/outbound.c:3250
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr "EXECKILL [-9], buộc kết thức tiến trình đang chạy trong phiên làm việc hiện "
"tại. Nếu thêm cờ « -9 » thì SIGKILL tiến trình này."

#: src/common/outbound.c:3252
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, gởi tiến trình SIGSTOP."

#: src/common/outbound.c:3253
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, gởi dữ liệu cho thiết bị gõ chuẩn của tiến trình — EXECute (thi "
"hành) WRITE (ghi)."

#: src/common/outbound.c:3257
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, _xóa sạch hàng đợi_ gởi của máy phục vụ hiện tại — Queue (hàng đời)."

#: src/common/outbound.c:3259
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr "GATE <máy_phục_vụ> [<cổng>], sử dụng máy ủy nhiệm; <cổng> mặc định là 23."

#: src/common/outbound.c:3262
msgid "GHOST <nick> <password>, Kills a ghosted nickname"
msgstr "GHOST <tên_hiệu> <mật_khẩu>, buộc kết thức tên hiệu _ma_ (bị ngắt còn máy "
"phục vụ chưa thay đổi trạng thái)"

#: src/common/outbound.c:3267
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr "HOP <tên_hiệu>, đưa ra <tên hiệu> trạng thái quản trị kênh nửa (cần quyền "
"quản trị kênh [chanop]) — Half (nửa) OPerator (quản trị viên)."

#: src/common/outbound.c:3268
msgid "ID <password>, identifies yourself to nickserv"
msgstr "ID <mật_khẩu>, _nhận diện_ bạn với máy phục vụ tên hiệu — IDentify"

#: src/common/outbound.c:3270
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr "IGNORE <bộ_lọc> <kiểu..> <tùy_chọn..> »\n"
"[ignore: bỏ qua]\n"
"    bộ lọc — bộ lọc máy cần bỏ qua, v.d. « *!*@*.aol.com »\n"
"    kiểu — kiểu dữ liệu cần bỏ qua, giá trị là một hay tất cả của:\n"
"           PRIV (riêng), CHAN (kênh), NOTI (thông báo), CTCP,\n"
"\t\t\tDCC (trò chuyện trực tiếp), INVI (mời), ALL (tất cả)\n"
"    tùy chọn — NOSAVE (không lưu), QUIET (không hiện chi tiết)"

#: src/common/outbound.c:3277
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr "INVITE <tên_hiệu> [<kênh>], _mời_ <tên hiệu> vào <kênh>; mặc định là kênh "
"hiện tại (cần quyền quản trị kênh [chanop])."

#: src/common/outbound.c:3278
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <kênh>, _vào_ <kênh>."

#: src/common/outbound.c:3280
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr "KICK <tên_hiệu>, _đá_ <tên hiệu> ra kênh hiện tại (cần quyền quản trị kênh "
"[chanop])."

#: src/common/outbound.c:3282
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr "KICKBAN <tên_hiệu>, _đuổi_ rồi _đá_ <tên hiệu> ra kênh hiện tại (cần quyền "
"quản trị kênh [chanop])."

#: src/common/outbound.c:3285
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, buộc _kiểm tra sự trễ_ tái kết nối."

#: src/common/outbound.c:3287
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <chuỗi>, tìm kiếm <chuỗi> trong bộ đệm (_bản ghi cuối cùng_)."

#: src/common/outbound.c:3289
msgid "LOAD [-e] <file>, loads a plugin or script"
msgstr "LOAD [-e] <tập_tin>, _tải_ một bộ cầm phít hay tập lệnh nào đó."

#: src/common/outbound.c:3292
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr "MDEHOP, bỏ trạng thái quản trị kênh nửa (chanhalf-op) ra mọi người trên kênh "
"hiện tại (cần quyền quản trị kênh [chanop]) — Mass (số nhiều) DE (bỏ) Half "
"(nửa) OPerator (quản trị viên)."

#: src/common/outbound.c:3294
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr "MDEOP, bỏ trạng thái quản trị kênh (chanop) ra mọi người trên kênh hiện tại "
"(cần quyền quản trị kênh [chanop]) — Mass (số nhiều) DE (bỏ) OPerator (quản "
"trị viên)"

#: src/common/outbound.c:3296
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr "ME <hành_động>, gởi <hành động> cho kênh hiện tại (v.d. « /me jumps » gởi « "
"<tên hiệu> nhảy » — ME (tôi, mình)."

#: src/common/outbound.c:3299
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr "MKICK, _đá_ mọi người (trừ bạn) ra kênh hiện tại (cần quyền quản trị kênh "
"[chanop]) — Mass (số nhiều)."

#: src/common/outbound.c:3302
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr "MOP, đưa ra mọi người dùng có trạng thái quản trị kênh (chanop) trên kênh "
"hiện tại (cần quyền quản trị kênh [chanop]) — Mass (số nhiều) OPerator (quản "
"trị viên)."

#: src/common/outbound.c:3303
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <tên_hiệu> <tin_nhẳn>, gởi <tin nhẳn> riêng — MeSsaGe (tin nhẳn)."

#: src/common/outbound.c:3306
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, liệt kê mọi _tên_ hiệu trên kênh hiện tại."

#: src/common/outbound.c:3308
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <tên_hiệu> <tin_nhẳn>, gởi thông báo CTCP — Notice (thông báo)."

#: src/common/outbound.c:3309
msgid "NEWSERVER [-noconnect] <hostname> [<port>]"
msgstr "NEWSERVER [-noconnect] <hostname> [<port>]"

#: src/common/outbound.c:3310
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <tên_hiệu>, đặt <tên hiệu> của bạn — NICKname (tên hiệu [viết tắt])."

#: src/common/outbound.c:3313
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr "NOTICE <tên_hiệu/kênh> <tin_nhản>, gởi một _thông báo_: thông báo là kiểu "
"tin nhẳn nên nhận trả lời tự động."

#: src/common/outbound.c:3315
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr "NOTIFY [<tên_hiệu>], liệt kê danh sách _thông báo_ của bạn, hoặc thêm người "
"nào vào nó."

#: src/common/outbound.c:3317
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr "OP <tên_hiệu>, đưa ra <tên hiệu> có trạng thái quản trị kênh (chanop) (cần "
"quyền quản trị kênh [chanop]) — OPerator (quản trị viên)."

#: src/common/outbound.c:3319
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<kênh>] [<lý_do>], rời <kênh> đi; mặc định là kênh hiện tại — dePART "
"(rời đi)."

#: src/common/outbound.c:3321
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <tên_hiệu | kênh>, thực hiện tiến trình « ping » CTCP với <tên hiêu> "
"hay <kênh>."

#: src/common/outbound.c:3323
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <tên_hiệu>, mở cửa sổ tin nhẳn riêng mới tới <tên hiệu> (_truy_vấn_)."

#: src/common/outbound.c:3325
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<lý_do>], ngắt kết nối ra máy phục vụ hiện tại (_thoát_)."

#: src/common/outbound.c:3327
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <đoạn>, gởi <đoạn> dạng thô cho máy phục vụ (_trích dẫn_)."

#: src/common/outbound.c:3330
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr "RECONNECT [-ssl] [<máy_phục_cụ>] [<cổng>] [<mật_khẩu>], có thể tái kết nối "
"đến mấy phục vụ hiện tại khi chỉ đơn giản bằng « /RECONNECT », hoặc tại kết "
"nối đến mọi máy phục vụ đang mở bằng « /RECONNECT ALL » (phương pháp SSL bảo "
"vệ dữ liệu cần truyền)."

#: src/common/outbound.c:3333
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr "RECONNECT [<máy_phục_vụ>] [<cổng>] [<mật_khẩu>], có thể tái kết nối đến mấy "
"phục vụ hiện tại chỉ đơn giản bằng « /RECONNECT », hay kết nối lại đến mọi "
"máy phục vụ đang mở bằng « /RECONNECT ALL » (không có SSL bảo vệ dữ liệu)."

#: src/common/outbound.c:3335
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr "RECV <đoạn>, gởi dữ liệu thô vào trình xchat, dạng do máy phục vụ IRC gởi — "
"RECeiVe (nhận)."

#: src/common/outbound.c:3338
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <đoạn>, gởi <đoạn> cho đối tượng trong cửa sổ hiện tại (_nói_)."

#: src/common/outbound.c:3339
msgid "SEND <nick> [<file>]"
msgstr "SEND <tên_hiệu> [<tập_tin>], _gởi_ tập tin cho tên hiệu"

#: src/common/outbound.c:3342
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <máy_phục_vụ> <cổng> <kênh>, kết nối đến <kênh> rồi vào nó "
"(phương pháp SSL bảo vệ dữ liệu sẽ truyền) — SERVer (máy phục vụ) CHANnel "
"(kênh)."

#: src/common/outbound.c:3345
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <máy_phục_vụ> <cổng> <kênh>, kết nối đến <kênh> rồi vào nó (không "
"có SSL bảo vệ dữ liệu) — SERVer (máy phục vụ) CHANnel (kênh)."

#: src/common/outbound.c:3349
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr "SERVER [-ssl] <máy_phục_vụ> [<cổng>] [<mật_khẩu>], kết nối đến <máy phục "
"vụ>: <cổng> mặc định là số 6667 cho kết nối chuẩn, và số 9999 cho kết nối "
"kiểu SSL (phương pháp SSL bảo vệ dữ liệu sẽ truyền)."

#: src/common/outbound.c:3352
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr "SERVER <máy_phục_vụ> [<cổng>] [<mật_khẩu>], kết nối đến <mấy phục vụ>; cổng "
"mặc định là số 6667 (không có SSL bảo vệ dữ liệu)."

#: src/common/outbound.c:3354
msgid "SET [-quiet] <variable> [<value>]"
msgstr "SET [-quiet] <biến> [<giá_trị>], _đặt_ giá trị cho <biến>; tùy chọn « -quiet "
"» (im) có nghĩa là không in ra chi tiết."

#: src/common/outbound.c:3355
msgid "SETCURSOR [-|+]<position>"
msgstr "SETCURSOR [-|+]<vị_trí>, _đặt_ <vị trí> cho _con trỏ_."

#: src/common/outbound.c:3359
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr "TOPIC [<chủ_đè>], lập <chủ đề> nếu nó đã cho, nếu không thì hiển thị chủ đề "
"hiện tại."

#: src/common/outbound.c:3361
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <bộ_lọc> [<bộ_lọc>...], _bỏ_ trạng thái _bị đuổi_ ra những bộ lọc được "
"gõ."

#: src/common/outbound.c:3362
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <bộ_lọc> [QUIET], _bỏ_ trạng thái _bị bỏ qua_ ra <bộ lọc> (tùy chọn "
"« QUIET » (im) có nghĩa là đừng in ra chi tiết)."

#: src/common/outbound.c:3363
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <tên>,_bỏ tải_ một bộ cầm phít hay tập lệnh nào đó."

#: src/common/outbound.c:3364
msgid "URL <url>, opens a URL in your browser"
msgstr "URL <địa_chỉ_Mạng>, mở <địa chỉ Mạng> trong trình duyệt Mạng của bạn."

#: src/common/outbound.c:3366
msgid ""
"USELECT [-a] [-s] <nick1> <nick2> etc, highlights nick(s) in channel userlist"
msgstr "USELECT [-a] [-s] <tên_hiệu1> <tên_hiệu2> v.v., nổi bật những tên hiệu này "
"trong danh sách các người dùng trên kênh — User (người dùng) SELECT (lựa "
"chọn)."

#: src/common/outbound.c:3369
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr "VOICE <tên_hiệu>, đưa ra <tên hiệu> trạng thái _tiếng nói_ (cần quyền quản "
"trị kênh [chanop])."

#: src/common/outbound.c:3371
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <tin_nhẳn>, ghi <tin nhẳn> vào mọi kênh — Write (ghi) ALL (mọị) "
"CHANnel (kênh)."

#: src/common/outbound.c:3373
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr "WALLCHOP <tin_nhẳn>, gởi <tin nhẳn> cho _mọi quản trị kênh [chanop]_ trên "
"kênh hiện tại — Write (ghi) ALL (mọi) CHannel (kênh) OPerator (quản trị "
"viên)."

